#include <iostream>
#include <thread>
#include <chrono>
#include <string>
int main() {
    std::string input;
    while (std::cin >> input) {
        std::string target = input;
        std::string result;
        for (char t : target) {
            for (char c = 32; c <= 126; ++c) {
                std::cout << "\r" << result << c << std::flush;
                std::this_thread::sleep_for(std::chrono::milliseconds(30));
                if (c == t) {
                    result += c;
                    break;
                }
            }
        }
    }
    }
